{
  "name": "friemt/deployer-tasks",
  "description": "Additional and enhanced tasks not available for Deployer out of the box.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Friemt\\Deployer\\": "src/"
    },
    "files": [
      "src/autoload.php"
    ]
  },
  "config": {
    "lock": false,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "ext-ctype": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "deployer/deployer": "^7.0",
    "symfony/console": "^5.4 || ^6.0",
    "symfony/polyfill-php80": "^1.26"
  },
  "require-dev": {},
  "scripts": {}
}
